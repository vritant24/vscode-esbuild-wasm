{
  "name": "esb",
  "displayName": "esb",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "browser": "./dist/web/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "esb.build",
        "title": "Build"
      }
    ]
  },
  "scripts": {
    "int-test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=dist/web/test/suite/index.js",
    "vscode:prepublish": "npm run package-web",
    "compile-web": "webpack",
    "watch-web": "webpack --watch",
    "package-web": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts",
    "run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. ./workspace",
    "test": "vitest -c ./vitest.config.js run"
  },
  "devDependencies": {
    "@edge-runtime/vm": "^2.1.2",
    "@types/assert": "^1.5.10",
    "@types/mocha": "^10.0.6",
    "@types/path-browserify": "^1.0.2",
    "@types/vscode": "^1.85.0",
    "@types/webpack-env": "^1.18.4",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vscode/test-web": "^0.0.22",
    "assert": "^2.1.0",
    "eslint": "^8.54.0",
    "mocha": "^10.2.0",
    "process": "^0.11.10",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "esbuild-wasm": "^0.19.9",
    "path-browserify": "^1.0.1",
    "vitest": "^1.0.1"
  }
}
